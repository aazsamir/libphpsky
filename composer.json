{
    "name": "aazsamir/libphpsky",
    "description": "PHP library for interacting with ATProto protocol",
    "autoload": {
        "psr-4": {
            "Aazsamir\\Libphpsky\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Examples\\": "examples/"
        }
    },
    "authors": [
        {
            "name": "aazsamir",
            "email": "sutikkukun@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "nette/php-generator": ">=4.1 <5.0",
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": ">=7.0 <8.0",
        "psr/cache": "^3.0",
        "symfony/cache": ">=7.0 <8.0",
        "amphp/amp": ">=3.0 <4.0",
        "amphp/http-client": ">=5.2 <6.0",
        "phrity/websocket": "^3.2"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.65",
        "symfony/var-dumper": "^7.2",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "license": "MIT",
    "scripts": {
        "qa": [
            "@phpcsfixer",
            "@rector",
            "@phpstan",
            "@phpunit"
        ],
        "phpcsfixer": "php-cs-fixer fix --diff --dry-run --verbose --allow-risky=yes",
        "phpstan": "phpstan analyse",
        "phpunit": "XDEBUG_MODE=coverage phpunit --coverage-text",
        "rector": "rector --dry-run",
        "integration-tests": "phpunit --testsuite integration"
    }
}
